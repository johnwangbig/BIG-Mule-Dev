#%RAML 1.0 DataType
type: object
properties:
  id: string
  name: string
  country?:   
    type: string
    enum:
    - CA
    - US
  postalcode?: 
    type: string
    pattern: \w{5,10}
  state?: string
  city?: string
example:
  id: 0013i0000075LHGAA2
  name: John
  country: CA
  postalcode: L1L1L1
  state: ON
  city: Toronto